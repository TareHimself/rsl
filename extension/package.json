{
  "name": "rsl",
  "displayName": "RSL",
  "description": "Vs Code Support for Rin Shading Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "rsl",
      "aliases": ["RSL", "rsl"],
      "extensions": [".rsl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rsl",
      "scopeName": "source.rsl",
      "path": "./syntaxes/rsl.tmLanguage.json"
    }]
  }
}
